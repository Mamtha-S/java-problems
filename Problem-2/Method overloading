package demo;
  
public class overloading {
	static int avg(int a,int b) {
		return (a+b)/2;
	}
	static int avg(int a,int b,int c) {
		return (a+b+c)/3;
	}
	public static void main(String[] args) {
		System.out.println(avg(5,7));
		System.out.println(avg(5,7,4));
	}

}
-----------------
OUTPUT
-----------------
6
5
